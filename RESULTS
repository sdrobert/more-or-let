#! /usr/bin/env bash
for x in exp/*/decode_*; do
	[ -d $x ] && echo $x | grep "${1:-.*}" > /dev/null && grep WER $x/wer_* /dev/null 2> /dev/null | utils/best_wer.sh
done
exit 0

